{
    "builds": [
        {
            "src": "client/vite.config.js",
            "use": "@vercel/vite",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "server/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/$1"
        },
        {
            "src": "/api/(.*)",
            "dest": "server/server.js"
        }
    ]
}
