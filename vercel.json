{
    "version": 2,
    "builds": [
      {
        "src": "client/vite.config.js",  
        "use": "@vercel/vite",
        "config": {
          "build": {
            "outDir": "client/dist"
          }
        }
      },
      {
        "src": "server/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",  
        "dest": "server/server.js"  
      },
      {
        "handle": "filesystem"
      }
    ]
  }